{
    "name": "parzival42codes/laravel-kint",
    "description": "Debugbar Kint",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^5.0|^6.0|^7.0",
        "spatie/laravel-package-tools": "^1.9",
        "spatie/php-structure-discoverer": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "parzival42codes\\LaravelKint\\": "src"
        },
        "files": [
            "src/App/Helpers/KintHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "parzival42codes\\LaravelKint\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "parzival42codes\\LaravelKint\\KintServiceProvider"
            ]
        }
    }
}
